.home
  @extend .container
  .jumbotron
    background none
    text-align center
    .work-in-progress
      margin: 0 auto 2em auto
      border-bottom: 1px solid #fff
      p
        color: white
        font-size: 1.5em
        text-shadow: 0px 0px 4px olive-green, 0px 0px 8px olive-green, 0px 0px 12px olive-green
        padding: 0
        margin: 0 auto 1em auto
        width: 90%
    .sub-title
      background green-rgba
      margin 0 auto
      padding 40px
      width 75%
